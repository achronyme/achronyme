// Type system and typeof

assert(typeof(42) == "Number")
assert(typeof(true) == "Bool")
assert(typeof(false) == "Bool")
assert(typeof(nil) == "Nil")
assert(typeof("hello") == "String")
assert(typeof("") == "String")
assert(typeof([]) == "List")
assert(typeof([1, 2]) == "List")
assert(typeof({}) == "Map")
assert(typeof({a: 1}) == "Map")
assert(typeof(field(1)) == "Field")

// typeof on expressions
assert(typeof(1 + 2) == "Number")
assert(typeof(true && false) == "Bool")
assert(typeof("a" + "b") == "String")

// typeof on fn
let f = fn(x) { x }
assert(typeof(f) == "Function")

