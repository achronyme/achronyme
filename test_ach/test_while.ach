
// Test While Loops

// 1. Basic Loop
mut i = 0
while i < 5 {
    print(i)
    i = i + 1
}
print(100)

// 2. Loop that doesn't run
while false {
    print(999) 
}
print(200)

// 3. Register Hygiene Stress in Loop
mut j = 0
while j < 3 {
    let a = 1
    let b = 2
    let c = 3
    print(j + a + b + c) // j + 6
    j = j + 1
}
print(300)
