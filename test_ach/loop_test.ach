print("--- Test: For List ---")
let list = [10, 20, 30]
for x in list {
    print(x)
}

print("--- Test: For Map ---")
// Keys order is not guaranteed, but they should be printed.
let map = {"a": 1, "b": 2}
for k in map {
    print(k)
}

print("--- Test: Break ---")
mut i = 0
while i < 10 {
    if i == 5 {
        break
    }
    print(i)
    i = i + 1
}

print("--- Test: Continue ---")
mut j = 0
while j < 5 {
    j = j + 1
    if j == 3 {
        continue
    }
    print(j)
}

print("--- Test: Forever ---")
mut k = 0
forever {
    k = k + 1
    if k > 3 {
        break
    }
    print(k)
}

print("--- Test: Nested Break ---")
for x in [1, 2] {
    print("Outer:")
    print(x)
    for y in [10, 20] {
        if y == 20 {
            break
        }
        print("Inner:")
        print(y)
    }
}
